{
	"name": "eslint-config-kylsgl",
	"version": "1.3.4",
	"description": "My personal ESLint rules for JavaScript, TypeScript, and React, based on the Airbnb JavaScript Style Guide.",
	"keywords": [
		"eslint",
		"eslintconfig",
		"eslint-config"
	],
	"homepage": "https://github.com/kylsgl/eslint-config-kylsgl-beta#readme",
	"bugs": {
		"url": "https://github.com/kylsgl/eslint-config-kylsgl-beta/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/kylsgl/eslint-config-kylsgl-beta"
	},
	"funding": [
		{
			"type": "ko-fi",
			"url": "https://ko-fi.com/kylsgl"
		}
	],
	"license": "MIT",
	"author": {
		"name": "kylsgl",
		"email": "hello@kylsgl.com",
		"url": "https://github.com/kylsgl"
	},
	"type": "module",
	"exports": {
		".": {
			"require": "./dist/index.js",
			"import": "./dist/index.mjs",
			"types": "./dist/index.d.ts"
		}
	},
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"./dist",
		"./CHANGELOG.md"
	],
	"scripts": {
		"build": "pnpm generate:rules && rollup --config ./configs/app/rollup.config.mjs",
		"build:eslint": "pnpm build && rollup --config ./configs/eslint/rollup.config.mjs",
		"format": "prettier --write .",
		"generate:rules": "tsx ./scripts/generateRules.ts",
		"lint": "eslint . --report-unused-disable-directives --max-warnings 0",
		"lint:fix": "pnpm lint --fix",
		"precommit": "pnpm lint:fix && pnpm format && pnpm test",
		"release": "pnpm build && changeset publish",
		"test": "tsc --noEmit && vitest --config ./configs/vitest/vitest.config"
	},
	"dependencies": {
		"@stylistic/eslint-plugin": "^4.4.1",
		"@vitest/eslint-plugin": "^1.2.7",
		"eslint-config-prettier": "^10.1.5",
		"eslint-import-resolver-typescript": "^4.4.3",
		"eslint-plugin-import-x": "^4.15.2",
		"eslint-plugin-jsx-a11y": "^6.10.2",
		"eslint-plugin-n": "^17.20.0",
		"eslint-plugin-package-json": "^0.40.4",
		"eslint-plugin-perfectionist": "^4.15.0",
		"eslint-plugin-promise": "^7.2.1",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "6.0.0-rc.1",
		"eslint-plugin-react-refresh": "^0.4.20",
		"eslint-plugin-regexp": "^2.9.0",
		"eslint-plugin-simple-import-sort": "^12.1.1",
		"eslint-plugin-sonarjs": "^3.0.3",
		"eslint-plugin-testing-library": "^7.5.3",
		"eslint-plugin-tsdoc": "^0.4.0",
		"eslint-plugin-unicorn": "^59.0.1",
		"typescript-eslint": "^8.34.1"
	},
	"devDependencies": {
		"@changesets/changelog-github": "^0.5.1",
		"@changesets/cli": "^2.29.5",
		"@types/estree": "^1.0.8",
		"@types/node": "^24.0.3",
		"eslint": "^9.29.0",
		"husky": "^9.1.7",
		"prettier": "^3.5.3",
		"rollup": "^4.44.0",
		"rollup-plugin-dts": "^6.2.1",
		"rollup-plugin-esbuild": "^6.2.1",
		"tsx": "^4.20.3",
		"typescript": "^5.8.3",
		"vitest": "^3.2.4"
	},
	"engines": {
		"node": ">=22",
		"pnpm": ">=10"
	}
}
